<script type="text/javascript">

    $(document).ready(function() {
        $("#id_valid_till").datepick({
           dateFormat: 'yyyy-mm-dd',
           changeMonth: true,
           changeYear: true,
           minDate:1,
           });

       $('#id_valid_from,#id_valid_to').datepick({ 
           dateFormat: 'yyyy-mm-dd',
           changeMonth: true,
           changeYear: true,
           minDate:0,
           });

        $("#id_expires").datepick({
           dateFormat: 'yyyy-mm-dd',
           changeMonth: true,
           changeYear: true,
           minDate:0,
           });
       });
</script>
<script type="text/javascript">
    function video_valid(){
        var date = new Date();
        var valid_till=document.getElementById('id_valid_till').value;
        alert(valid_till);
        if (valid_till < new Date(date.getFullYear(), date.getMonth(), date.getDate()))
        {
            alert('Valid till must be greater than today.');
            return false;
        }
        return true;
}
	
</script>
